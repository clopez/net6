#: src/error.cpp:46
msgid "An unknown login error occured"
msgstr "Si è verificato un errore di accesso sconosciuto"

#: src/error.cpp:394
msgid "Interrupted function call"
msgstr "Chiamata di funzione interrotta"

#: src/error.cpp:408
msgid "Socket operation on non-socket"
msgstr "Operazione per socket su non-socket"

#: src/error.cpp:410
msgid "Destination address required"
msgstr "Richiesto indirizzo di destinazione"

#: src/error.cpp:414
msgid "Protocol wrong type for socket"
msgstr "Tipo errato di protocollo per il socket"

#: src/error.cpp:432
msgid "Network is down"
msgstr "La rete non è disponibile"

#: src/error.cpp:434
msgid "Network is unreachable"
msgstr "La rete non è raggiungibile"

#: src/error.cpp:436
msgid "Network dropped connection on reset"
msgstr "Connessione interrotta da una reinizializzazione"

#: src/error.cpp:438
msgid "Software caused connection abort"
msgstr "Connessione interrotta per un problema software"

#: src/error.cpp:440
msgid "Connection reset by peer"
msgstr "Connessione interrotta dal corrispondente"

#: src/error.cpp:444
msgid "Socket is already connected"
msgstr "Socket già connesso"

#: src/error.cpp:446
msgid "Socket is not connected"
msgstr "Socket non connesso"

#: src/error.cpp:448
msgid "Cannot send after socket shutdown"
msgstr "Impossibile inviare dopo l'arresto del socket"

#: src/error.cpp:454
msgid "Host is down"
msgstr "L'host è disattivato"

#: src/error.cpp:460
msgid "Network subsystem is unavailable"
msgstr "Il sottosistema di rete non è disponibile"

#: src/error.cpp:462
msgid "Winsock.dll version out of range"
msgstr "Versione di winsock.dll fuori misura"

#: src/error.cpp:464
msgid "Successful WSAStartup not yet performed"
msgstr "WSAStartup non è stato ancora eseguito con successo"

#: src/error.cpp:466
msgid "Graceful shutdown in progress"
msgstr "Procedura di graceful shutdown in corso"

#: src/error.cpp:472
msgid "Nonauthoritative host not found"
msgstr "Host non-authoritative non trovato"

#: src/error.cpp:474
msgid "No data record of requested type"
msgstr "Nessun record di dati del tipo richiesto"

#: src/error.cpp:476
msgid "Specified event object handle is invalid"
msgstr "Gestione dell'oggetto dell'evento specificato non valida"

#: src/error.cpp:482
msgid "Broken pipe"
msgstr "Pipe rotta"

#: src/error.cpp:484
msgid "No such device"
msgstr "Device inesistente"

#: src/error.cpp:528
msgid "A nonrecoverable error has occured"
msgstr "Si è verificato un errore irreversibile"

#: src/error.cpp:42
msgid "Invalid name"
msgstr "Nome non valido"

#: src/error.cpp:44
msgid "Name is already in use"
msgstr "Il nome è già in uso"

#: src/error.cpp:396
msgid "Access denied"
msgstr "Accesso negato"

#: src/error.cpp:398
msgid "Bad address"
msgstr "Indirizzo errato"

#: src/error.cpp:400
msgid "Invalid argument"
msgstr "Argomento non valido"

#: src/error.cpp:402
msgid "Too many open files"
msgstr "Troppi file aperti"

#: src/error.cpp:404
msgid "Resource temporarily unavailable"
msgstr "Risorsa temporaneamente non disponibile"

#: src/error.cpp:406
msgid "Operation already in progress"
msgstr "Operazione già in corso"

#: src/error.cpp:412
msgid "Message too long"
msgstr "Messaggio troppo lungo"

#: src/error.cpp:416
msgid "Bad protocol option"
msgstr "Opzione protocollo errata"

#: src/error.cpp:418
msgid "Protocol not supported"
msgstr "Protocollo non supportato"

#: src/error.cpp:420
msgid "Socket type not supported"
msgstr "Tipo di socket non supportato"

#: src/error.cpp:422
msgid "Operation not supported"
msgstr "Operazione non supportata"

#: src/error.cpp:424
msgid "Protocol family not supported"
msgstr "Famiglia di protocolli non supportata"

#: src/error.cpp:426
msgid "Address family not supported"
msgstr "Famiglia di indirizzi non supportata"

#: src/error.cpp:428
msgid "Address is already in use"
msgstr "Indirizzo già in uso"

#: src/error.cpp:430
msgid "Cannot assign requested address"
msgstr "Impossibile assegnare l'indirizzo richiesto"

#: src/error.cpp:442
msgid "No buffer space available"
msgstr "Nessuno spazio di buffer disponibile"

#: src/error.cpp:450
msgid "Connection timed out"
msgstr "Connessione terminata"

#: src/error.cpp:452
msgid "Connection refused"
msgstr "Connessione rifiutata"

#: src/error.cpp:456
msgid "No route to host"
msgstr "Nessun instradamento per l'host"

#: src/error.cpp:458
msgid "Too many processes"
msgstr "Troppi processi"

#: src/error.cpp:468
msgid "Class type not found"
msgstr "Tipo di classe non trovata"

#: src/error.cpp:470
msgid "Host not found"
msgstr "Host non trovato"

#: src/error.cpp:478
msgid "One or more parameters are invalid"
msgstr "Uno o più parametri non sono validi"

#: src/error.cpp:480
msgid "No more memory is available"
msgstr "Non è disponibile altra memoria"

#: src/error.cpp:486
msgid "Decryption has failed"
msgstr "Decifratura fallita"

#: src/error.cpp:488
msgid ""
"The Diffie Hellman prime sent by the server is not acceptable (not long "
"enough)"
msgstr ""
"Il numero primo Diffie Hellman trasmesso dal server non è accettabile (non "
"abbastanza lungo)"

#: src/error.cpp:491
msgid "Encryption has failed"
msgstr "Cifratura fallita"

#: src/error.cpp:493
msgid "TLS Application data were received, while expecting handshake data"
msgstr "Dati TLS Application ricevuti, in attesa dei dati di handshake"

#: src/error.cpp:496
msgid "Insufficient credentials for that request"
msgstr "Credenziali non sufficienti per quella richiesta"

#: src/error.cpp:498
msgid "The request is invalid"
msgstr "La richiesta non è valida"

#: src/error.cpp:500
msgid "Key usage violation in certificate has been detected"
msgstr ""
"È stata riscontrata una violazione di utilizzo della chiave nel certificato"

#: src/error.cpp:503
msgid "The Message Authentication Code verification failed"
msgstr "La verifica del codice per l'autenticazione del messaggio è fallita"

#: src/error.cpp:506
msgid "The peer did not send any certificate"
msgstr "Nessun certificato trasmesso dal corrispondente"

#: src/error.cpp:508
msgid "No temporary DH parameters were found"
msgstr "Non sono stati trovati parametri DH temporanei"

#: src/error.cpp:510
msgid "No temporary RSA parameters were found"
msgstr "Non sono stati trovati parametri RSA temporanei"

#: src/error.cpp:512
msgid "Error in the pull function"
msgstr "Errore nella funzione pull"

#: src/error.cpp:514
msgid "Error in the push function"
msgstr "Errore nella funzione push"

#: src/error.cpp:516
msgid "Failed to acquire random data"
msgstr "Acquisizione dati casuali fallita"

#: src/error.cpp:518
msgid "Rehandshake was requested by the peer"
msgstr "Un nuovo handshake è stato richiesto dal corrispondente"

#: src/error.cpp:520
msgid "An unexpected TLS handshake packet was received"
msgstr "È stato ricevuto un pacchetto handshake TLS non atteso"

#: src/error.cpp:523
msgid "An unexpected TLS packet was received"
msgstr "È stato ricevuto un pacchetto TLS imprevisto"

#: src/error.cpp:525
msgid "A TLS packet with unexpected length was received."
msgstr "È stato ricevuto un pacchetto TLS di lunghezza imprevista."
