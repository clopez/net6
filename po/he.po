#: src/error.cpp:42
msgid "Invalid name"
msgstr "שם שגוי"

#: src/error.cpp:44
msgid "Name is already in use"
msgstr "השם כבר נמצא בשימוש"

#: src/error.cpp:46
msgid "An unknown login error occured"
msgstr "ארעה שגיאה לא ידועה בהתחברות"

#: src/error.cpp:394
msgid "Interrupted function call"
msgstr "הקריאה לפונקציה נקטעה"

#: src/error.cpp:396
msgid "Access denied"
msgstr "הגישה נדחית"

#: src/error.cpp:398
msgid "Bad address"
msgstr "כתובת שגויה"

#: src/error.cpp:400
msgid "Invalid argument"
msgstr "ארגומנט שגוי"

#: src/error.cpp:402
msgid "Too many open files"
msgstr "יותר מדי קבצים פתוחים"

#: src/error.cpp:404
msgid "Resource temporarily unavailable"
msgstr "המשאב אינו זמין באופן זמני"

#: src/error.cpp:406
msgid "Operation already in progress"
msgstr "הפעולה כבר מתבצעת"

#: src/error.cpp:408
msgid "Socket operation on non-socket"
msgstr "פעולת שקע או לא-שקע"

#: src/error.cpp:410
msgid "Destination address required"
msgstr "כתובת היעד נדרשת"

#: src/error.cpp:412
msgid "Message too long"
msgstr "ההודעה ארוכה מדי"

#: src/error.cpp:414
msgid "Protocol wrong type for socket"
msgstr "סוג פרוטוקול שגוי עבור השקע"

#: src/error.cpp:416
msgid "Bad protocol option"
msgstr "אפשרות פרוטוקול בעייתית"

#: src/error.cpp:418
msgid "Protocol not supported"
msgstr "הפרוטוקול אינו נתמך"

#: src/error.cpp:420
msgid "Socket type not supported"
msgstr "סוג השקע אינו נתמך"

#: src/error.cpp:422
msgid "Operation not supported"
msgstr "הפעולה אינה נתמכת"

#: src/error.cpp:424
msgid "Protocol family not supported"
msgstr "משפחת הפרוטקול אינה נתמכת"

#: src/error.cpp:426
msgid "Address family not supported"
msgstr "משפחת הכתובות אינה נתמכת"

#: src/error.cpp:428
msgid "Address is already in use"
msgstr "הכתובת כבר נמצאת בשימוש"

#: src/error.cpp:430
msgid "Cannot assign requested address"
msgstr "לא ניתן להקצות את הכתובת המבוקשת"

#: src/error.cpp:432
msgid "Network is down"
msgstr "הרשת נפלה"

#: src/error.cpp:434
msgid "Network is unreachable"
msgstr "הרשת אינה נגישה"

#: src/error.cpp:436
msgid "Network dropped connection on reset"
msgstr "חיבור הרשת נקטע עם האיתחול"

#: src/error.cpp:438
msgid "Software caused connection abort"
msgstr "התוכנה גרמה לביטול החיבור"

#: src/error.cpp:440
msgid "Connection reset by peer"
msgstr "החיבור אותחל על ידי העמית"

#: src/error.cpp:442
msgid "No buffer space available"
msgstr "אין נפח פנוי לחוצץ"

#: src/error.cpp:444
msgid "Socket is already connected"
msgstr "השקע כבר מחובר"

#: src/error.cpp:446
msgid "Socket is not connected"
msgstr "השקע אינו מחובר"

#: src/error.cpp:448
msgid "Cannot send after socket shutdown"
msgstr "לא ניתן לשלוח לאחר כיבוי השקע"

#: src/error.cpp:450
msgid "Connection timed out"
msgstr "זמן החיבור פג"

#: src/error.cpp:452
msgid "Connection refused"
msgstr "החיבור נדחה"

#: src/error.cpp:454
msgid "Host is down"
msgstr "המארח נפל"

#: src/error.cpp:456
msgid "No route to host"
msgstr "אין נתיב למארח"

#: src/error.cpp:458
msgid "Too many processes"
msgstr "יותר מדי תהליכים"

#: src/error.cpp:460
msgid "Network subsystem is unavailable"
msgstr "תת-מערכת הרשת אינה זמינה"

#: src/error.cpp:462
msgid "Winsock.dll version out of range"
msgstr "גירסת ה-Winsock.dll אינה בטווח"

#: src/error.cpp:464
msgid "Successful WSAStartup not yet performed"
msgstr "WSAStartup מוצלח לא בוצע עדיין"

#: src/error.cpp:466
msgid "Graceful shutdown in progress"
msgstr "מתבצע כיבוי חינני"

#: src/error.cpp:468
msgid "Class type not found"
msgstr "סוג המחלקה לא נמצא"

#: src/error.cpp:470
msgid "Host not found"
msgstr "המארח לא נמצא"

#: src/error.cpp:472
msgid "Nonauthoritative host not found"
msgstr "לא נמצא מארח ללא סמכות"

#: src/error.cpp:474
msgid "No data record of requested type"
msgstr "אין רשומת נתונים עבור הסוג המבוקש"

#: src/error.cpp:476
msgid "Specified event object handle is invalid"
msgstr "מזהה אירוע האובייקט המצויין אינו תקני"

#: src/error.cpp:478
msgid "One or more parameters are invalid"
msgstr "פרמטר אחד או יותר אינם תקניים"

#: src/error.cpp:480
msgid "No more memory is available"
msgstr "אין עוד זיכרון זמין"

#: src/error.cpp:482
msgid "Broken pipe"
msgstr "משרשר קטוע"

#: src/error.cpp:484
msgid "No such device"
msgstr "אין כזה התקן"

#: src/error.cpp:486
msgid "Decryption has failed"
msgstr "הפענוח נכשל"

#: src/error.cpp:488
msgid ""
"The Diffie Hellman prime sent by the server is not acceptable (not long "
"enough)"
msgstr ""
"המספר הראשוני המוגדר על ידי פרוטוקול דיפי הלמן הנשלח על ידי השרת אינו מקובל "
"(לא ארוך מספיק)"

#: src/error.cpp:491
msgid "Encryption has failed"
msgstr "ההצפנה נכשלה"

#: src/error.cpp:493
msgid "TLS Application data were received, while expecting handshake data"
msgstr "נתוני תוכנית ה-TLS התקבלו, בזמן שנתוני לחיצת-יד היו אמורים להגיע"

#: src/error.cpp:496
msgid "Insufficient credentials for that request"
msgstr "זיהוי לא מספק עבור בקשה זו"

#: src/error.cpp:498
msgid "The request is invalid"
msgstr "הבקשה אינה תקנית"

#: src/error.cpp:500
msgid "Key usage violation in certificate has been detected"
msgstr "זוהתה הפרה בשימוש מפתח עבור התעודה"

#: src/error.cpp:503
msgid "The Message Authentication Code verification failed"
msgstr "אישור קוד אימות ההודעה נכשל"

#: src/error.cpp:506
msgid "The peer did not send any certificate"
msgstr "העמית לא שלח הודעה כלשהי"

#: src/error.cpp:508
msgid "No temporary DH parameters were found"
msgstr "לא נמצאו פרמטרים זמניים של דיפי הלמן"

#: src/error.cpp:510
msgid "No temporary RSA parameters were found"
msgstr "לא נמצאו פרמטרים זמניים של RSA"

#: src/error.cpp:512
msgid "Error in the pull function"
msgstr "שגיאה בפעולת המשיכה"

#: src/error.cpp:514
msgid "Error in the push function"
msgstr "שגיאה בפעולת הדחיפה"

#: src/error.cpp:516
msgid "Failed to acquire random data"
msgstr "שגיאה באיחזור נתונים אקראיים"

#: src/error.cpp:518
msgid "Rehandshake was requested by the peer"
msgstr "נתבקשה לחיצת-יד מחודשת על ידי העמית"

#: src/error.cpp:520
msgid "An unexpected TLS handshake packet was received"
msgstr "התקבלה חבילת לחיצת יד TLS לא צפויה"

#: src/error.cpp:523
msgid "An unexpected TLS packet was received"
msgstr "התקבלה חבילת TLS לא צפויה"

#: src/error.cpp:525
msgid "A TLS packet with unexpected length was received."
msgstr "התקבלה חבילת TLS בעלת אורך לא צפוי."

#: src/error.cpp:528
msgid "A nonrecoverable error has occured"
msgstr "התגלתה שגיאה שאינה ניתנת להחלמה"
