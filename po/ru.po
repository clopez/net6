#: src/error.cpp:42
msgid "Invalid name"
msgstr "Неправильное имя"

#: src/error.cpp:44
msgid "Name is already in use"
msgstr "Такое имя уже используется"

#: src/error.cpp:46
msgid "An unknown login error occured"
msgstr "Неизвестная ошибка логина"

#: src/error.cpp:394
msgid "Interrupted function call"
msgstr "Прерванный вызов функции"

#: src/error.cpp:396
msgid "Access denied"
msgstr "Доступ запрещён"

#: src/error.cpp:398
msgid "Bad address"
msgstr "Неправильный адрес"

#: src/error.cpp:400
msgid "Invalid argument"
msgstr "Неверный аргумент"

#: src/error.cpp:402
msgid "Too many open files"
msgstr "Слишком много открытых файлов"

#: src/error.cpp:404
msgid "Resource temporarily unavailable"
msgstr "Ресурс временно недоступен"

#: src/error.cpp:406
msgid "Operation already in progress"
msgstr "Операция уже выполняется"

#: src/error.cpp:408
msgid "Socket operation on non-socket"
msgstr "Операция с сокетом не на сокете"

#: src/error.cpp:410
msgid "Destination address required"
msgstr "Требуется адрес назначения"

#: src/error.cpp:412
msgid "Message too long"
msgstr "Сообщение слишком длинное"

#: src/error.cpp:414
msgid "Protocol wrong type for socket"
msgstr "Неверный тип протокола для сокета"

#: src/error.cpp:416
msgid "Bad protocol option"
msgstr "Неверный параметр протокола"

#: src/error.cpp:418
msgid "Protocol not supported"
msgstr "Протокол не поддерживается"

#: src/error.cpp:420
msgid "Socket type not supported"
msgstr "Тип сокета не поддерживается"

#: src/error.cpp:422
msgid "Operation not supported"
msgstr "Операция не поддерживается"

#: src/error.cpp:424
msgid "Protocol family not supported"
msgstr "Семейство протоколов не поддерживается"

#: src/error.cpp:426
msgid "Address family not supported"
msgstr "Семейство адресов не поддерживается"

#: src/error.cpp:428
msgid "Address is already in use"
msgstr "Адрес уже используется"

#: src/error.cpp:430
msgid "Cannot assign requested address"
msgstr "Невозможно присвоить запрошенный адрес"

#: src/error.cpp:432
msgid "Network is down"
msgstr "Сеть не функционирует"

#: src/error.cpp:434
msgid "Network is unreachable"
msgstr "Сеть недоступна"

#: src/error.cpp:436
msgid "Network dropped connection on reset"
msgstr "Соединение с сетью утеряно при сбросе"

#: src/error.cpp:438
msgid "Software caused connection abort"
msgstr "Программа вызвала разрыв соединения"

#: src/error.cpp:440
msgid "Connection reset by peer"
msgstr "Узел разорвал соединение"

#: src/error.cpp:442
msgid "No buffer space available"
msgstr "Нет места для буфера"

#: src/error.cpp:444
msgid "Socket is already connected"
msgstr "Сокет уже подключён"

#: src/error.cpp:446
msgid "Socket is not connected"
msgstr "Сокет не подключён"

#: src/error.cpp:448
msgid "Cannot send after socket shutdown"
msgstr "Невозможно отправить данные после отключения сокета"

#: src/error.cpp:450
msgid "Connection timed out"
msgstr "Время ожидания соединения истекло"

#: src/error.cpp:452
msgid "Connection refused"
msgstr "Соединение отвергнуто"

#: src/error.cpp:454
msgid "Host is down"
msgstr "Узел не отвечает"

#: src/error.cpp:456
msgid "No route to host"
msgstr "Нет маршрута к узлу"

#: src/error.cpp:458
msgid "Too many processes"
msgstr "Слишком много процессов"

#: src/error.cpp:460
msgid "Network subsystem is unavailable"
msgstr "Сетевая подсистема недоступна"

#: src/error.cpp:462
msgid "Winsock.dll version out of range"
msgstr "Версия Winsock.dll вне требуемого диапазона"

#: src/error.cpp:464
msgid "Successful WSAStartup not yet performed"
msgstr "Успешный WSAStartup ещё не произведён"

#: src/error.cpp:466
msgid "Graceful shutdown in progress"
msgstr "Выполняется мягкое выключение"

#: src/error.cpp:468
msgid "Class type not found"
msgstr "Тип класса не найден"

#: src/error.cpp:470
msgid "Host not found"
msgstr "Узел не найден"

#: src/error.cpp:472
msgid "Nonauthoritative host not found"
msgstr "Не доверительный узел не найден"

#: src/error.cpp:474
msgid "No data record of requested type"
msgstr "Нет записей данных запрошенного типа"

#: src/error.cpp:476
msgid "Specified event object handle is invalid"
msgstr "Выбранный обработчик объекта событий неверен"

#: src/error.cpp:478
msgid "One or more parameters are invalid"
msgstr "Один или несколько параметров неверны"

#: src/error.cpp:480
msgid "No more memory is available"
msgstr "Доступной памяти больше нет"

#: src/error.cpp:482
msgid "Broken pipe"
msgstr "Канал повреждён"

#: src/error.cpp:484
msgid "No such device"
msgstr "Устройство не существует"

#: src/error.cpp:486
msgid "Decryption has failed"
msgstr "Не удалось расшифровать"

#: src/error.cpp:488
msgid ""
"The Diffie Hellman prime sent by the server is not acceptable (not long "
"enough)"
msgstr ""
"Посланное сервером простое число Diffie Hellman неприемлемо (недостаточно "
"длинное)"

#: src/error.cpp:491
msgid "Encryption has failed"
msgstr "Зашифровать не удалось"

#: src/error.cpp:493
msgid "TLS Application data were received, while expecting handshake data"
msgstr ""
"Получены программные данные (по TLS), но ожидались данные TLS-рукопожатия "
"(handshake)"

#: src/error.cpp:496
msgid "Insufficient credentials for that request"
msgstr "Недостаточно полномочий для данного запроса"

#: src/error.cpp:498
msgid "The request is invalid"
msgstr "Недопустимый запрос"

#: src/error.cpp:500
msgid "Key usage violation in certificate has been detected"
msgstr "Обнаружено нарушение использования ключа в сертификате"

#: src/error.cpp:503
msgid "The Message Authentication Code verification failed"
msgstr "Проверка кода аутентичности сообщения (MAC) не удалась"

#: src/error.cpp:506
msgid "The peer did not send any certificate"
msgstr "Узел не послал сертификат"

#: src/error.cpp:508
msgid "No temporary DH parameters were found"
msgstr "Не найдены временные параметры DH"

#: src/error.cpp:510
msgid "No temporary RSA parameters were found"
msgstr "Не найдены временные параметры RSA"

#: src/error.cpp:512
msgid "Error in the pull function"
msgstr "Ошибка функции pull"

#: src/error.cpp:514
msgid "Error in the push function"
msgstr "Ошибка функции push"

#: src/error.cpp:516
msgid "Failed to acquire random data"
msgstr "Не удалось получить случайные данные"

#: src/error.cpp:518
msgid "Rehandshake was requested by the peer"
msgstr "Узел затребовал повторное рукопожатие (rehandshake)"

#: src/error.cpp:520
msgid "An unexpected TLS handshake packet was received"
msgstr "Был получен неожиданный пакет TLS-рукопожатия (handshake)"

#: src/error.cpp:523
msgid "An unexpected TLS packet was received"
msgstr "Был получен неожиданный пакет TLS"

#: src/error.cpp:525
msgid "A TLS packet with unexpected length was received."
msgstr "Был получен пакет TLS с неожиданной длиной."

#: src/error.cpp:528
msgid "A nonrecoverable error has occured"
msgstr "Возникла неисправимая ошибка"
